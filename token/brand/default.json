        },
        "medium": {
          "$type": "color",
          "$value": "{colors.blue.20}",
          "$description": "Tertiary background color is used for prominent semantics, generally for selection, info, etc."
        },
        "default": {
          "$type": "color",
          "$value": "{colors.blue.5}",
          "$description": " Primary background color for informative component."
