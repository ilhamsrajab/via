{
  "actions": {
    "cta-button": {
      "primary": {
        "default": {
          "container": {
            "border-color": {
              "$type": "color",
              "$value": "{brand-colors.background.default}"
            },
            "background-color": {
              "$type": "color",
              "$value": "{brand-colors.border.default}"
            }
          }
        }
      },
      "standard": {
        "rest": {
          "container": {
            "border-width": {
              "$type": "borderWidth",
              "$value": "{dimensions.border-width.m}"
            },
            "border-radius": {
              "$type": "borderRadius",
              "$value": "{dimensions.radius.m}"
            },
            "padding": {
              "$type": "spacing",
              "$value": "{theme-spacing.padding.s} {theme-spacing.padding.l}"
            }
          }
        }
      }
    }
  }
}